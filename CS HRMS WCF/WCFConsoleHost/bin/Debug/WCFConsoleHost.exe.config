				</endpoint>
				<!--<endpoint address="net.tcp://localhost:8001/TCP_EmpService" binding="netTcpBinding" contract="WCFEmpServiceLib.IEmpService"/>-->
				
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
