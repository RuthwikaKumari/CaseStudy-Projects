        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	
	<connectionStrings>
		<add name="sqlconstr" 
			 connectionString="Data Source=IN-5CG0253H0K\GDATA2014;Initial Catalog=DXCDB;Integrated Security=True" 
